{
  "compilerOptions": {
    "rootDir": "./",
    "target": "es5",
    "module": "commonjs",
    "lib": ["esnext"],
    "jsx": "react",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}